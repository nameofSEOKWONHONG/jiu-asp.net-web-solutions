<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="RecStop">Stop Recording</system:String>
    <system:String x:Key="RecTime">Recording Time</system:String>
    <system:String x:Key="RecUploadTitle">Screencast (UPLOAD)</system:String>

    <system:String x:Key="RecReg">Save(F8)</system:String>
    <system:String x:Key="RecExit">Close</system:String>

    <system:String x:Key="Caption">Info.</system:String>
    <system:String x:Key="ErrorCaption">Error</system:String>
